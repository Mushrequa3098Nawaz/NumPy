        "import math\n",
        "def r_linspace(lower, upper, number):\n",
        "\ta=[]\n",
        "\tnumber=math.floor(number)\n",
        "\tif number==1:\n",
        "\t\ta.append(round(lower,8))\n",
        "\t\treturn a\n",
        "\tnumber-=1\n",
        "\tstep=(upper-lower)/number\n",
        "\twhile round(lower,8)<=upper:\n",
        "\t\ta.append(round(lower,8))\n",
        "\t\tlower+=step\n",
        "\treturn a"
      ],
      "execution_count": 0,
